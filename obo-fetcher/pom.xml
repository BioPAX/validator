<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <modelVersion>4.0.0</modelVersion>
  <artifactId>obo-fetcher</artifactId>

  <parent>
	<artifactId>biopax-validator</artifactId>
	<groupId>org.biopax.validator</groupId>
	<version>3.0.0-SNAPSHOT</version>
  </parent>
 
  <name>Ontologies Access</name>

  <description>Provides OBO ontology access and handling. It is derived from EBI / PSIDEV Ontology Manager and Ontology Lookup Service sources</description>

	<repositories>
		<repository>
			<id>ebi-repo</id>
			<name>The EBI internal repository</name>
			<url>http://www.ebi.ac.uk/~maven/m2repo</url>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
	</repositories>

    <dependencies>
        <dependency>
        	<groupId>obo</groupId>
        	<artifactId>obo</artifactId>
        	<version>2.0</version>
        </dependency>
        <dependency>
        	<groupId>obo</groupId>
        	<artifactId>bbop</artifactId>
        	<version>2.0</version>
        </dependency>
        <dependency>
        	<groupId>proteomics</groupId>
        	<artifactId>proteomics-common</artifactId>
        	<version>1.5</version>
        </dependency>
        <dependency>
            <groupId>opensymphony</groupId>
            <artifactId>oscache</artifactId>
            <version>2.4</version>
            <exclusions>
            	<exclusion>
            		<artifactId>jms</artifactId>
            		<groupId>javax.jms</groupId>
            	</exclusion>
            </exclusions>
        </dependency>
    </dependencies>
  
</project>